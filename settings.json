{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "chat.commandCenter.enabled": false,
  "editor.codeActionsOnSave": {},
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "block",
  "editor.cursorSurroundingLines": 20,
  "editor.cursorWidth": 10,
  "editor.fastScrollSensitivity": 2,
  "editor.fontFamily": "'Hack', 'Droid Sans Mono', 'Jetbrains Mono', 'Consolas'",
  "editor.fontLigatures": false,
  "editor.fontSize": 16,
  "editor.fontWeight": 400,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": true,
  "editor.lineDecorationsWidth": 15, // Not actually unknown :)
  "editor.lineHeight": 2.6,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.stickyScroll.enabled": false,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "errorLens.enabled": true,
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "info",
    "error",
    "hint"
  ],
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "files.autoSave": "onWindowChange",
  "files.trimTrailingWhitespace": true,
  "json.schemas": [],
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorStyleInactive": "block",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.smoothScrolling": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "windowed",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "bottom",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#c5c5c515",
    "editor.lineHighlightBorder": "#ffffff00"
  },
  "workbench.colorTheme": "Cosmic Night",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.labelFormat": "medium",
  "workbench.list.smoothScrolling": true,
  "workbench.settings.applyToAllProfiles": [],
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.iconTheme": "bearded-icons",
}